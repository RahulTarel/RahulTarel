package array.logic;

public class ReverseNumberArray {

	public static void main(String[] args) {
		System.out.print("enter numbers in array to reverse print it");

		int left = 0, right = array.length - 1;
		while (left < right) {
			int temp = array[left];
			array[left] = array[right];
			array[right] = temp;
			left++;
			right--;
		}
	}
}
